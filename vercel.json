{
    "version": 2,
    "builds": [
      {
        "src": "appweather/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }
      },
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/delete/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
          {
            "src": "/api",
            "dest": "api/index.py"
        },
        {
          "src": "/api/delete",
          "dest": "api/delete/index.py"
      },
        {
          "src": "/(.*)",
          "dest": "appweather/wsgi.py"
        }
    ]
  }